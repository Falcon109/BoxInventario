<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.BoxInventario"
        tools:targetApi="31">
        <activity
            android:name=".clases.CrearProductos"
            android:exported="false"
            android:label="@string/CrearProducto"
            android:parentActivityName=".clases.ApartadoArticulos">
            <meta-data
            android:name="android.suport.PARENT_ACTIVITY"
            android:value="ApartadoArticulos" />
        </activity>
        <activity
            android:name=".clases.DescipcionProductos"
            android:exported="false"
            android:label="@string/DescripcionProducto"
            android:parentActivityName=".clases.ApartadoArticulos">
            <meta-data
                android:name="android.suport.PARENT_ACTIVITY"
                android:value="ApartadoArticulos" />
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:exported="false"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".entidades.Usuario"
            android:exported="true"
            android:label="@string/VentanaUsuario">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".clases.Recordatorios"
            android:exported="false"
            android:label="@string/VentanaRecordatorio"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.suport.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".clases.ApartadoLugarDeInteres"
            android:exported="false"
            android:label="@string/VentanaLugar"
            android:parentActivityName=".MainActivity">
            >
            <meta-data
                android:name="android.suport.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".clases.ApartadoListas"
            android:exported="false"
            android:label="@string/VentanaListas"
            android:parentActivityName=".MainActivity">
            >
            <meta-data
                android:name="android.suport.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".clases.ApartadoArticulos"
            android:exported="false"
            android:label="@string/VentanaArticulos"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.suport.PARENT_ACTIVITY"
                android:value="MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>