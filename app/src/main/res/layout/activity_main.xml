<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Cuerpo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color003"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#0075644B"
        android:contentDescription="Usuario"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/usuario_foreground" />

    <TextView
        android:id="@+id/ApartadoListas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="216dp"
        android:text="@string/TituloLista"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TituloMenu" />

    <TextView
        android:id="@+id/TituloMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="27dp"
        android:text="@string/TituloMenu"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/ApartadoListas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toEndOf="@+id/imageButton4"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/BotonLista"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="38dp"
        android:background="@color/articulo_background"
        android:contentDescription="@string/BotonLista"
        android:saveEnabled="false"
        app:layout_constraintBottom_toTopOf="@+id/ApartadoLugares"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ApartadoListas"
        app:srcCompat="@drawable/articulo_foreground" />

    <ImageButton
        android:id="@+id/BotonLugares"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="32dp"
        android:background="@color/articulo_background"
        android:contentDescription="@string/BotonLugares"
        android:saveEnabled="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ApartadoLugares"
        app:srcCompat="@drawable/lugar_foreground" />

    <TextView
        android:id="@+id/ApartadoLugares"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="31dp"
        android:text="@string/TituloUbicaciones"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/BotonLugares"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BotonLista" />

    <ImageButton
        android:id="@+id/BotonAgregarListas"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="141dp"
        android:backgroundTint="@color/agregar_background"
        android:contentDescription="@string/BotonAgregarLista"
        app:layout_constraintBottom_toTopOf="@+id/ApartadoLugares"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/BotonLista"
        app:layout_constraintTop_toBottomOf="@+id/ApartadoListas"
        app:layout_constraintVertical_bias="0.073"
        app:srcCompat="@drawable/agregar_foreground" />

    <ImageButton
        android:id="@+id/BotonAgregarLugares"
        android:layout_width="108dp"
        android:layout_height="108dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="141dp"
        android:backgroundTint="@color/agregar_background"
        android:contentDescription="@string/BotonAgregarLugares"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/BotonLugares"
        app:layout_constraintTop_toBottomOf="@+id/ApartadoLugares"
        app:srcCompat="@drawable/agregarlugar_foreground" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="77dp"
        android:layout_height="77dp"
        android:layout_marginStart="316dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:background="@color/color002"
        android:contentDescription="BotonNotificaciones"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/notificaciones_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>